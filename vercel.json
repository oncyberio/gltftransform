{
  "redirects": [
    {
      "source": "/modules/functions{.html}?",
      "destination": "/functions.html"
    },
    {
      "source": "/:group/:type(\\w+){.html}?",
      "destination": "/:group/core.:type.html"
    }
  ],
  "rewrites": [
    {
      "source": "/:page(concepts|contributing|extensions|cli|credits){.html}?",
      "destination": "/modules/pages.:page.html"
    },
    {
      "source": "/functions{.html}?",
      "destination": "/modules/functions.html"
    }
  ]
}
