<section class="tsd-panel tsd-member {{cssClasses}}">
    <a name="{{anchor}}" class="tsd-anchor"></a>

    {{#if signatures}}
        {{> member.signatures}}
    {{else}}{{#if hasGetterOrSetter}}
        {{> member.getterSetter}}
    {{else}}{{#if tryGetTargetReflectionDeep}}
        {{> member.reference}}
    {{else}}
        {{> member.declaration}}
    {{/if}}{{/if}}{{/if}}

    {{#unless isContainer}}
        {{#each groups}}
            {{#each children}}
                {{#unless hasOwnDocument}}
                    {{> member}}
                {{/unless}}
            {{/each}}
        {{/each}}
    {{/unless}}
</section>

{{#if isContainer}}
    {{> members}}
{{/if}}
